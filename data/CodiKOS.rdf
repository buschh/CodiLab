    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">CodiKOS</skos:prefLabel>
    <skos:prefLabel xml:lang="de">CodiKOS</skos:prefLabel>
    <skos:notation>CodiKOS</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Codicological SKOS data model</skos:definition>
      <skos:definition xml:lang="de">Kodikologisches SKOS Datenmodell</skos:definition>
    </skos:note>
    <skos:narrower rdf:resource="http://www.ecodicology.org/0001"/>
