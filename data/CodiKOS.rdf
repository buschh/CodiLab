  <rdf:Description rdf:about="http://www.ecodicology.org/0003">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Pricking</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Einstechen</skos:prefLabel>
    <skos:notation>0003</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The marking of a folio or bifolium by a point or knife to guide ruling.</skos:definition>
      <skos:definition xml:lang="de">Markierung eines Blattes oder einer Lage mittels kleiner Einstiche als Orientierung für das Liniennetz und den Linierungsrahmen.</skos:definition>
    </skos:note>
    <skos:narrower rdf:resource="http://www.ecodicology.org/0051"/>
    <skos:narrower rdf:resource="http://www.ecodicology.org/0052"/>
    <skos:narrower rdf:resource="http://www.ecodicology.org/0053"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0004">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Ruling</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Linierung</skos:prefLabel>
    <skos:notation>0004</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The process by which a frame and/or horizontal lines are produced (via drawing, scratching or folding) to guide the hand in writing.</skos:definition>
      <skos:definition xml:lang="de">Prozess der Linierung, Anlegen des Hilfsliniennetzes (Rahmen oder horizontale Schreiblinien) durch Vorzeichnung, Vorritzung oder Knicken/Falten der Seite.</skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0005">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Layout</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Seitenlayout</skos:prefLabel>
    <skos:notation>0005</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">refers to the layout of a page which means the general arrangement of the various elements on a page</skos:definition>
      <skos:definition xml:lang="de">bezieht sich auf die Seitengestaltung, die allgemeine Einteilung und Aufteilung der unterschiedlichen Elemente auf der Seite</skos:definition>
    </skos:note>
    <skos:narrower rdf:resource="http://www.ecodicology.org/331.07"/>
    <skos:narrower rdf:resource="http://www.ecodicology.org/331.071"/>
    <skos:narrower rdf:resource="http://www.ecodicology.org/331.09"/>
    <skos:narrower rdf:resource="http://www.ecodicology.org/333.02"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/331.07">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Margin</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Randbereich</skos:prefLabel>
    <skos:notation>331.07</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">space outside the writing frame</skos:definition>
      <skos:definition xml:lang="de">Raum außerhalb des Beschriftungsrahmens</skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0009">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Illumination</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Buchmalerei</skos:prefLabel>
    <skos:notation>0009</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">the embellishment of a manuscript with luminous colours (especially gold and silver), i.e. the total of decorative elements and pictorial representations</skos:definition>
      <skos:definition xml:lang="de">Verschönerung der Handschrift mit leuchtenden Farben (v.a. Gold und Silber); die Gesamtheit der dekorativen Elemente und bildlichen Darstellungen</skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0027">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Page</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Seite</skos:prefLabel>
    <skos:notation>0027</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Each face of a folio.</skos:definition>
      <skos:definition xml:lang="de">Jede Seitenfläche eines Blattes.</skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/331.071">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Inner Margin</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Bundsteg</skos:prefLabel>
    <skos:notation>331.071</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">inner marginal area of the page</skos:definition>
      <skos:definition xml:lang="de">innerer Rand der Seite (zum Bund hin)</skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/331.09">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Body of page</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Schriftraum</skos:prefLabel>
    <skos:notation>331.09</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">everything within the writing space/ has to be distinguished from marginal elements</skos:definition>
      <skos:definition xml:lang="de">gesamte Fläche innerhalb des Beschriftungsraumes, muss von Randelementen unterschieden werden</skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/333.02">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Initial</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Initiale</skos:prefLabel>
    <skos:notation>333.02</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">enlarged and decorated letter, introducing an important section of a text (work, chapter), emphasised by size, colour form, and decoration. Initials can have different levels of significance (primary, secondary, tertiary)</skos:definition>
      <skos:definition xml:lang="de">schmückender Buchstabe am Textanfang, hervorgehoben durch Größe, Farbe, Form und Verzierung, leitet einen hervorzuhebenden Abschnitt eines Textes (Werk, Kapitel) ein. Initialen haben verschiedene hierarchische Bedeutungsebenen (primär, sekundär, tertiär)</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/511.01"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0006">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Margin / Marginal Notes</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Marginalien</skos:prefLabel>
    <skos:notation>0006</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en"></skos:definition>
      <skos:definition xml:lang="de"></skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0008">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Script</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Schrift</skos:prefLabel>
    <skos:notation>0008</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en"></skos:definition>
      <skos:definition xml:lang="de"></skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0007">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Initial</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Initiale</skos:prefLabel>
    <skos:notation>0007</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">enlarged and decorated letter, introducing an important section of a text (work, chapter), emphasised by size, colour form, and decoration. Initials can have different levels of significance (primary, secondary, tertiary)</skos:definition>
      <skos:definition xml:lang="de">schmückender Buchstabe am Textanfang, hervorgehoben durch Größe, Farbe, Form und Verzierung, leitet einen hervorzuhebenden Abschnitt eines Textes (Werk, Kapitel) ein. Initialen haben verschiedene hierarchische Bedeutungsebenen (primär, sekundär, tertiär)</skos:definition>
    </skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0010">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Codex</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Kodex</skos:prefLabel>
    <skos:notation>0010</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">A book composed of folded sheets sewn along one edge. Includes everything within the binding/cover.</skos:definition>
      <skos:definition xml:lang="de">Ein Buch, das aus gefalteten Bögen besteht, die in der Mitte zusammengenäht sind. Beinhaltet ist alles innerhalb der Bindung.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0011">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Collected manuscript, composite manuscript</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Sammelhandschrift</skos:prefLabel>
    <skos:notation>0011</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Codex with more than one text, e.g. different authors or genres. May be conceptional but mostly random for aims of conservation.</skos:definition>
      <skos:definition xml:lang="de">Heterogene Handschriften, in denen Texte unterschiedlicher Autoren oder Textsorten zu einem Kodex zusammengeführt sind. Sowohl konzeptionell als Sammlung oder wahllos aus Konservierungszwecken.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0012">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Volume</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Band</skos:prefLabel>
    <skos:notation>0012</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Unit, consisting of a set of sheets bound together; part of a homogeneous text.</skos:definition>
      <skos:definition xml:lang="de">Einheit, aus einer Reihe von zusammengebundenen Bögen bestehend; Teil eines homogenen Textes.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0013">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Book block</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Buchblock</skos:prefLabel>
    <skos:notation>0013</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Bound folios without cover, assembled in sequence (part between the boards), consists of several quires.</skos:definition>
      <skos:definition xml:lang="de">Alle Blätter eines Buches mit Ausnahme des Einbandes (Teil zwischen den Buchdeckeln); besteht aus mehreren Lagen.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0014">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Sheet</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Bogen</skos:prefLabel>
    <skos:notation>0014</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Whole piece of parchment or paper, not folded.</skos:definition>
      <skos:definition xml:lang="de">Ein ganzes ungefaltetes Stück Pergament oder Papier.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0015">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Quire / Gathering</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Lage</skos:prefLabel>
    <skos:notation>0015</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">A number of bifolia folded together; a set of bifolia nested one on top of the other. Quires are the 'gatherings' or 'booklets' of which a book is formed.</skos:definition>
      <skos:definition xml:lang="de">Besteht aus mehreren ineinandergelegten Doppelblättern. Hieraus ergeben sich Einzelheftchen, aus denen sich ein Buch zusammensetzt.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
    <skos:related rdf:resource="http://www.ecodicology.org/0016"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0016">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Bifolium</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Bifolium, Doppelblatt</skos:prefLabel>
    <skos:notation>0016</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Rectangular piece of parchment folded in the middle/in half to produce two leaves (i.e., four pages). A number of bifolia folded together form a quire.</skos:definition>
      <skos:definition xml:lang="de">Rechteckiges Stück Pergament, in der Mitte zur Hälfte gefaltet, um zwei Blätter (i.e. 4 Seiten) zu erhalten. Eine Gruppe gefalteter Bifolia ergibt eine Lage.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
    <skos:related rdf:resource="http://www.ecodicology.org/0015"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0017">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Endpaper / Front end paper (FEP)</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Vorsatzpapierbogen</skos:prefLabel>
    <skos:notation>0017</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Leaves of paper inserted before the title page and after the text. Doublepage consisting of Pastedown and Flyleaf.</skos:definition>
      <skos:definition xml:lang="de">Zwei oder mehr Blätter, die am Anfang oder Ende des Buches eingefügt sind und Buchblock und Einband verbinden.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0018">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Front (Head)</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Titelei</skos:prefLabel>
    <skos:notation>0018</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The top of a book / codex / manuscript, may include the first pages found at the start before the main body of the text, including front end papers.</skos:definition>
      <skos:definition xml:lang="de">Umfasst alle Seiten des Buchblocks / der Handschrift vom Vorsatzpapier bis zum eigentlichen Beginn des Haupttextes.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0019">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Back (Tail)</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Hinterer Teil, (Anhang)</skos:prefLabel>
    <skos:notation>0019</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Last pages of a manuscript / volume / codex. Contained are any appendixes following the main part of the text, including end papers.</skos:definition>
      <skos:definition xml:lang="de">Das hindere Ende / die letzten Seiten einer Handschrift / eines Bandes, anschließend an den Haupttext auch: Anhang.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0020">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Insertion</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Einfügung</skos:prefLabel>
    <skos:notation>0020</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Additional element added to a quire.</skos:definition>
      <skos:definition xml:lang="de">Zusätzliches Element, Zusatz zur Lage.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0021">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Fragment / Detached piece, Cut-out leaf</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Fragment, Blattverlust</skos:prefLabel>
    <skos:notation>0021</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Leafs / pieces of parchment remaining of a text which has been cut out, which is intentionally separated from the codex or accidentially lost.</skos:definition>
      <skos:definition xml:lang="de">Pergamentteile oder -blätter als Überreste eines verlorenen Textes, die absichtlich oder unabsichtlich aus dem Kodex herausgetrennt wurden.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0022">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Leaf / Folio</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Blatt</skos:prefLabel>
    <skos:notation>0022</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">One leaf, half / quarter / eigth of a sheet; one half of a bifolium. The front and back of a folio are referred to as the recto and verso.</skos:definition>
      <skos:definition xml:lang="de">Eine Hälfte / ein Viertel / ein Achtel eines Bogens bzw. ein halbes Bifolium. Die Vorder- und Rückseite eines Blattes werden mit recto und verso bezeichnet.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0023">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Opening</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Doppelseite, Doppelseitenansicht</skos:prefLabel>
    <skos:notation>0023</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Ensemble / together of two facing pages when the codex is opened.</skos:definition>
      <skos:definition xml:lang="de">Ansicht zweier gegenüberliegender Seiten beim Öffnen des Buches.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0024">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Flyleaf</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Leerblatt, Vorsatzblatt bzw. Nachstoßblatt</skos:prefLabel>
    <skos:notation>0024</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Protective leaf inserted between pastedown and first page, at the beginning or end of the book.</skos:definition>
      <skos:definition xml:lang="de">Schutzblatt, das zwischen Einbandspiegel und erster bzw. letzter Buchseite eingefügt wurde.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0025">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Thrown-out leaf / Fold</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Faltblatt</skos:prefLabel>
    <skos:notation>0025</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Leaf with larger dimensions that has been folded lengthwise and crosswise to fit in the codex.</skos:definition>
      <skos:definition xml:lang="de">Blatt größerer Höhe und Breite, das an der Längs- und Breitseite gefaltet wurde, um in den Kodex zu passen (dépliant).</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0026">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Guard-leaf / Protective leaf</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Vorblatt</skos:prefLabel>
    <skos:notation>0026</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Leaf / leaves before title page.</skos:definition>
      <skos:definition xml:lang="de">Ein der Titelseite vorgeschaltetes Blatt bzw. der Titelseite vorgeschaltete Blätter.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0028">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Recto</skos:prefLabel>
    <skos:prefLabel xml:lang="de">recto, Vorderseite (Blatt)</skos:prefLabel>
    <skos:notation>0028</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The front side of the leaf; one that appears first in the sense of reading the text.</skos:definition>
      <skos:definition xml:lang="de">Vordere Fläche bzw. Vorderseite eines Blattes, die sich beim aufgeschlagenen Buch auf der rechten Seite befindet.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0029">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Verso</skos:prefLabel>
    <skos:prefLabel xml:lang="de">verso, Rückseite (Blatt)</skos:prefLabel>
    <skos:notation>0029</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The back side of the leaf.</skos:definition>
      <skos:definition xml:lang="de">Hintere Fläche bzw. Rückseite eines Blattes, die sich im aufgeschlagenen Buch auf der linken Seite befindet.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0030">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Facing page</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Gegenüberliegende Seite</skos:prefLabel>
    <skos:notation>0030</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">A page which forms a double page with a given page; one half of a double page ( > opposite page).</skos:definition>
      <skos:definition xml:lang="de">Seite, die mit der jeweils gegenüberliegenden Seite eine Doppelseite bildet.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0031">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Singleton, Half sheet</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Einzelblatt</skos:prefLabel>
    <skos:notation>0031</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">A single folio that has lost its facing mate (the other half of the bifolium) or that originally was designed to be sewn into a book as a single page.</skos:definition>
      <skos:definition xml:lang="de">Seite ohne gegenüberliegende Seite. Hälfte eines Bifoliums, eventuell nachträglich eingefügt.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0032">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Pastedown</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Einbandspiegel</skos:prefLabel>
    <skos:notation>0032</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">A leaf pasted onto the inside of a board, that means the inner surface of the book cover.</skos:definition>
      <skos:definition xml:lang="de">Ein Blatt, das auf der Innenseite des Buchdeckels festgeklebt wurde.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0033">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Book mark</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Blattweiser</skos:prefLabel>
    <skos:notation>0033</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Strip of parchment or thin object, attached to the fore edge of the book at appropriate points in order to mark key openings.</skos:definition>
      <skos:definition xml:lang="de">Pergament- oder Papierstreifen, am Oberschnitt festgemacht; ermöglicht einen Schnellzugriff auf bestimmte Seiten.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0034">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Seal</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Siegel</skos:prefLabel>
    <skos:notation>0034</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Small mass of wax or metal which has been printed on a document to signify authentication or consent.</skos:definition>
      <skos:definition xml:lang="de">Kleine Wachs- oder Metallmenge, in flüssigem Zustand auf Papier gedruckt, kennzeichnet dessen Beglaubigung oder Bewilligung.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0035">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Stamping / Impression</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Stempel</skos:prefLabel>
    <skos:notation>0035</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Inscription or symbol to be inked and placed on volumes to mark their affiliation.</skos:definition>
      <skos:definition xml:lang="de">Inschrift oder Symbol, mit Tinte oder Farbe auf das Papier gedruckt (Abdruck), kennzeichnet die Zugehörigkeit.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0036">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Watermark</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Wasserzeichen</skos:prefLabel>
    <skos:notation>0036</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Distinguishing mark or design on paper, visible only when held up to the light; drawing whose footprint is left in the paper sheet by a wire forming a figure.</skos:definition>
      <skos:definition xml:lang="de">Charakteristisches Zeichen bzw. Symbol auf dem Papier, nur sichtbar, wenn gegen das Licht gehalten; mit einem gebogenen Draht aufgebracht.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0037">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Reinforcing strip</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Bindungsverstärkung</skos:prefLabel>
    <skos:notation>0037</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Strip of parchment folded in half along its length and placed on the fold, on the outside or the center of the book.</skos:definition>
      <skos:definition xml:lang="de">Pergamentstreifen, zur Hälfte längs gefaltet, am Falz, an der Außenseite oder innerhalb des Buches befestigt. Oft ein Fragment.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0038">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Foliation</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Blattzählung</skos:prefLabel>
    <skos:notation>0038</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Numbering of folios.</skos:definition>
      <skos:definition xml:lang="de">Nummerierung der Blätter.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0039">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Pagination</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Seitenzählung, Paginierung</skos:prefLabel>
    <skos:notation>0039</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Numbering of pages.</skos:definition>
      <skos:definition xml:lang="de">Nummerierung der Seiten.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0040">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Spine</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Buchrücken</skos:prefLabel>
    <skos:notation>0040</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The edge at which a book is sewn together; length of the volume saddle, corresponding to the cover of the book.</skos:definition>
      <skos:definition xml:lang="de">Die Seite, an der das Buch gebunden wurde; Länge des Buchrückens, gemessen am Bucheinband.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0041">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Fore edge</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Buchschnitt, Vorderschnitt</skos:prefLabel>
    <skos:notation>0041</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The edge of a book opposite the spine, sometimes decorated.</skos:definition>
      <skos:definition xml:lang="de">Vorderkante des Buches, liegt dem Buchrücken gegenüber; z. T. dekoriert.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0042">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Folding / Format</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Faltung</skos:prefLabel>
    <skos:notation>0042</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Indication of size by number of times the sheet has been folded (2, 4, 8 times).</skos:definition>
      <skos:definition xml:lang="de">Größenangabe, definiert anhand der Anzahl der Bogenfaltungen (2-, 4-, 8fach).</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0001"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0043">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Format</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Buchformat</skos:prefLabel>
    <skos:notation>0043</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Book size in width and height, usually given in millimetres.</skos:definition>
      <skos:definition xml:lang="de">Größenangabe für ein Buch in Breite und Höhe, gewöhnlich in Millimetern angegeben.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0002"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0044">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Folio-sized</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Folioformat</skos:prefLabel>
    <skos:notation>0044</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Term designating a folding method for the leaves.</skos:definition>
      <skos:definition xml:lang="de">Angabe der Faltungsmethode für die Blätter.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0002"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0045">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Oblong</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Querformat</skos:prefLabel>
    <skos:notation>0045</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Format more width than height.</skos:definition>
      <skos:definition xml:lang="de">Format, bei dem die Breite größer als die Höhe ist.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0002"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0046">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Height / Width of page</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Seitengröße</skos:prefLabel>
    <skos:notation>0046</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Vertical / Horizontal dimension of the whole codex page.</skos:definition>
      <skos:definition xml:lang="de">Vertikale / Horizontale Außmaße der Kodexseite.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0002"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0047">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Height / Width of Justification</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Schriftraumgröße</skos:prefLabel>
    <skos:notation>0047</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The surface dimensions of the written area.</skos:definition>
      <skos:definition xml:lang="de">Maße des beschrifteten Raumes.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0002"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0048">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Height / Width of pictoral space</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Bildraumgröße</skos:prefLabel>
    <skos:notation>0048</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The surface dimensions of the illustrated area.</skos:definition>
      <skos:definition xml:lang="de">Maße des bebilderten Raumes.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0002"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0049">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Interlinear space</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Zeilenabstand</skos:prefLabel>
    <skos:notation>0049</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Space between written lines.</skos:definition>
      <skos:definition xml:lang="de">Raum zwischen den Zeilen.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0002"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0050">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Intercolumn</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Spaltenabstand</skos:prefLabel>
    <skos:notation>0050</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Space between two adjacent columns.</skos:definition>
      <skos:definition xml:lang="de">Raum zwischen zwei angrenzenden Spalten.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0002"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0051">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Prick, Prick hole</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Einstich</skos:prefLabel>
    <skos:notation>0051</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Little insertion into the leaf for the alignment of the ruling frame.</skos:definition>
      <skos:definition xml:lang="de">Kleiner Einstich in das Blatt für die Ausrichtung des Linierungsnetzes.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0003"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0052">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Ruling prick</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Einstich für Linierung</skos:prefLabel>
    <skos:notation>0052</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Single prick on the far right of the page which determines the distances of the later writing lines.</skos:definition>
      <skos:definition xml:lang="de">Einstichlich an der rechten Seite des Blattes als Markierung für die Ausrichtung der späteren Schreiblinien.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0003"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0053">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Set of pricks</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Einstichreihe</skos:prefLabel>
    <skos:notation>0053</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Pricks / holes with the same alignment.</skos:definition>
      <skos:definition xml:lang="de">Gruppe von Einstichen mit der selben Funktion.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0003"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0054">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Ruling pattern (see also “Ruling system”)</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Zeilengerüst, Linierungsraster/-muster</skos:prefLabel>
    <skos:notation>0054</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Certain symmetrical pattern formed by the lines that make up for ruling (on only one page or across all pages/a several pages of a book).</skos:definition>
      <skos:definition xml:lang="de">Bestimmtes symmetrisches Muster der Linierungslinien (auf einer Seite, mehreren Seiten/über alle Seiten hinweg) = Resultat der Linierung.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0055">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Limit lines, Justification</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Schriftspiegel</skos:prefLabel>
    <skos:notation>0055</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Circumscription with limiting lines to delimit the text block.</skos:definition>
      <skos:definition xml:lang="de">Zur Beschriftung vorgesehene Fläche(n) einer Seite, inklusive ‘lebender Kolumnentitel’.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0056">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Writing frame</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Beschriftungsrahmen</skos:prefLabel>
    <skos:notation>0056</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Four lines limiting the justification on every page; can be divided into columns.</skos:definition>
      <skos:definition xml:lang="de">Vier Linien, die den Schriftspiegel begrenzen (ausgenommen Kolumnentitel); kann in Spalten aufgeteilt sein.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0057">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Frame line</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Rahmenlinie</skos:prefLabel>
    <skos:notation>0057</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Term for single lines of the writing frame: top line – bottom line – margin rule.</skos:definition>
      <skos:definition xml:lang="de">Bezeichnung für die einzelne Linie der Umrahmung: obere Begrenzungslinie, untere Begrenzungslinie, seitliche Begrenzungslinie.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0058">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Framing</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Gitternetz</skos:prefLabel>
    <skos:notation>0058</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Ensemble of frame lines, surrounding the justification on three or four sides.</skos:definition>
      <skos:definition xml:lang="de">Gesamtheit der Rahmenlinien, die die Beschriftungsfläche an drei oder vier Seiten begrenzen.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0059">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Intercolumn</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Begrenzung des Spaltenabstands</skos:prefLabel>
    <skos:notation>0059</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Two or more vertical lines to define the space which separates two columns.</skos:definition>
      <skos:definition xml:lang="de">Zwei oder mehr vertikale Linien, die den freien Raum zwischen zwei Spalten definieren.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0060">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Decentralized intercolumn</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Ungleichmäßige Spaltenbreite</skos:prefLabel>
    <skos:notation>0060</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">The distance between the columns is defined irregularly with respect to the central axis of the justification. The width of the text columns varies in consequence of the fitting to the predefined writing space.</skos:definition>
      <skos:definition xml:lang="de">Der Spaltenzwischen- raum ist asymmetrisch zur vertikalen Mittel- achse des Schriftraums angelegt. Die Breite der Textspalten variiert aufgrund der Anpassung in den vorgegebenen Schriftraum.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0061">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Varying intercolumn</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Variabler Spaltenabstand</skos:prefLabel>
    <skos:notation>0061</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Varying distance of intercolumnial lines on every page; the width of intercolumnial space varies from page to page.</skos:definition>
      <skos:definition xml:lang="de">Die Breite des Spaltenzwischenraumes variiert von Seite zu Seite, definiert durch die unregelmäßigen Abstand der Begrenzungslinien zueinander.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0062">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Occasional intercolumn</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Unvollständige Spaltenzwischenraum- linierung</skos:prefLabel>
    <skos:notation>0062</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Intercolumnial lines do not occupy the whole page height.</skos:definition>
      <skos:definition xml:lang="de">Die Trennlinien zwischen den Spalten, die den Zwischenraum definieren, nehmen nicht die gesamte Höhe der Textspalte ein.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0063">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Double / Triple ruling</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Doppellinierung, Dreifachlinierung</skos:prefLabel>
    <skos:notation>0063</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Two or more parallel lines separated to a minor distance of each other with the same function.</skos:definition>
      <skos:definition xml:lang="de">Zwei oder mehr parallele, eng zusammenliegende Linien mit gleicher Funktion.</skos:definition>
    </skos:note>
    <skos:broader rdf:resource="http://www.ecodicology.org/0004"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.ecodicology.org/0064">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Line ruling</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Zeilenlinierung</skos:prefLabel>
    <skos:notation>0064</skos:notation>
    <skos:note>
      <skos:definition xml:lang="en">Horizontal aid lines within the writing space.</skos:definition>
      <skos:definition xml:lang="de">Linierung mit horizontalen Schreiblinien innerhalb des beschrifteten Raumes.</skos:definition>
    </skos:note>
  </rdf:Description>
</rdf:RDF>